{
    "shutdownCmd": "/sbin/shutdown -h now",
    "convertPath": "/usr/bin/convert",
    "intervalDelay": 5,
    "intervalInterval": 10,
    "intervalShots": 250,
    "cameraSettings": [
        {
            "model": "nikon.*(d7000|d5100)",
            "params": [
                {
                    "category": "settings",
                    "param": "capturetarget",
                    "value": "Memory card"
                },
                {
                    "category": "imgsettings",
                    "param": "imagesize",
                    "value": "2464x1632"
                },
                {
                    "category": "capturesettings",
                    "param": "imagequality",
                    "value": "NEF+Basic"
                }
            ]
        }
    ]
}